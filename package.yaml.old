name: minecraft-dashboard
version: 0.1.0.0
license: BSD3
author: RishRishTheFish
maintainer: rishay.dutt@student.onslow.school.nz

dependencies:
  - base >= 4.7 && < 5
  - text
  - bytestring
  - directory
  - filepath
  - aeson
  - scotty
  - transformers

executables:
  minecraft-dashboard-exe:
    main: Main.hs
    source-dirs: src
    ghc-options:
      - -threaded
      - -rtsopts
      - -with-rtsopts=-N